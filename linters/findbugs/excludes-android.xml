<?xml version="1.0"?>

<FindBugsFilter>
    <Match>
        <Class name="~.*R\$.*" />
    </Match>
    <Match>
        <Class name="~.*Manifest\$.*" />
    </Match>
    <Match>
        <Class name="~.*\.*Test" />
        <Not>
            <Bug code="IJU" />
        </Not>
    </Match>
</FindBugsFilter>
